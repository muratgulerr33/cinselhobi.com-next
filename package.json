{
  "name": "cinselhobi-com-next",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "TZ=Europe/Istanbul next dev",
    "dev:3000": "TZ=Europe/Istanbul next dev -p 3000",
    "dev:3010": "PORT=3010 TZ=Europe/Istanbul next dev",
    "build": "TZ=Europe/Istanbul next build",
    "start": "TZ=Europe/Istanbul next start",
    "lint": "eslint",
    "db:generate": "drizzle-kit generate",
    "db:migrate": "drizzle-kit migrate",
    "db:check": "bash scripts/db/db_check.sh",
    "db:psql": "bash -lc 'source scripts/dev/load_env.sh && psql \"$DATABASE_URL\"'",
    "woo:import": "node --import tsx scripts/woo-import.ts",
    "categories:report": "node --import tsx scripts/category-detective-report.ts",
    "categories:hub-todo": "node --import tsx scripts/export-toplevel-hub-todo.ts",
    "products:smart-detective": "node --import tsx scripts/smart-products-detective.ts",
    "hub:qc": "node --import tsx scripts/hub-lock-qc.ts",
    "hub:verify": "node --import tsx scripts/verify-hub-map.ts",
    "category:lock": "node --import tsx scripts/category-tree-analysis.ts",
    "guardrail:forbidden": "node --import tsx scripts/guardrail-forbidden.ts",
    "seo:export": "node --import tsx scripts/export-product-seo-bundle.ts",
    "eroshop:image-map": "node --import tsx scripts/eroshop-image-map.ts",
    "eroshop:image-download": "node --import tsx scripts/eroshop-image-download.ts",
    "eroshop:image-apply": "node --import tsx scripts/eroshop-image-apply.ts",
    "export:catalog": "node --import tsx scripts/export_catalog.ts --out seo_exports/nextjs/v1",
    "export:category-stats": "node --import tsx scripts/export_category_stats.ts --out seo_exports/nextjs/v1",
    "export:seo:v1": "npm run export:catalog && npm run export:category-stats",
    "seo:nginx:v1": "python3 scripts/seo/gen_nginx_rules.py --redirects-csv seo_exports/nextjs/v1/redirects/wp_redirects.v1.csv --gone-txt seo_exports/nextjs/v1/removals/spam_410_paths.v1.txt --out-dir seo_exports/nextjs/v1/nginx",
    "seo:v1:all": "npm run export:seo:v1 && npm run seo:nginx:v1",
    "seo:deploy:nginx:v1": "bash scripts/seo/deploy_nginx_snippets_v1.sh",
    "seo:v1:ship": "npm run seo:v1:all && npm run seo:deploy:nginx:v1",
    "seo:test:v1": "python3 scripts/seo/smoke_test_v1.py",
    "gen:icons": "node scripts/generate-icons.mjs"
  },
  "dependencies": {
    "@auth/drizzle-adapter": "^1.11.1",
    "@hookform/resolvers": "^5.2.2",
    "@radix-ui/react-accordion": "^1.2.12",
    "@radix-ui/react-alert-dialog": "^1.1.15",
    "@radix-ui/react-avatar": "^1.1.11",
    "@radix-ui/react-dropdown-menu": "^2.1.16",
    "@radix-ui/react-label": "^2.1.8",
    "@radix-ui/react-navigation-menu": "^1.2.14",
    "@radix-ui/react-separator": "^1.1.8",
    "@radix-ui/react-slot": "^1.2.4",
    "@radix-ui/react-switch": "^1.2.6",
    "@radix-ui/react-visually-hidden": "^1.2.4",
    "@types/nodemailer": "^7.0.4",
    "argon2": "^0.44.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "drizzle-orm": "^0.45.1",
    "embla-carousel-react": "^8.6.0",
    "framer-motion": "^12.23.26",
    "lucide-react": "^0.562.0",
    "nanoid": "^5.0.9",
    "next": "16.1.1",
    "next-auth": "^5.0.0-beta.25",
    "next-themes": "^0.4.6",
    "nodemailer": "^7.0.12",
    "pg": "^8.16.3",
    "react": "19.2.3",
    "react-dom": "19.2.3",
    "react-hook-form": "^7.71.1",
    "sonner": "^2.0.7",
    "tailwind-merge": "^3.4.0",
    "tw-animate-css": "^1.4.0",
    "vaul": "^1.1.2",
    "zod": "^4.3.6"
  },
  "devDependencies": {
    "@tailwindcss/postcss": "^4",
    "@tailwindcss/typography": "^0.5.19",
    "@types/node": "^20",
    "@types/pg": "^8.16.0",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "dotenv": "^17.2.3",
    "drizzle-kit": "^0.31.8",
    "eslint": "^9",
    "eslint-config-next": "16.1.1",
    "png-to-ico": "^3.0.1",
    "sharp": "^0.33.5",
    "tailwindcss": "^4",
    "tsx": "^4.21.0",
    "typescript": "^5"
  }
}
